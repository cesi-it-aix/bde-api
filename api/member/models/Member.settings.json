{
  "connection": "default",
  "collectionName": "members",
  "info": {
    "name": "member",
    "description": "Office member"
  },
  "options": {
    "increments": true,
    "timestamps": [
      "createdAt",
      "updatedAt"
    ],
    "comment": ""
  },
  "attributes": {
    "name": {
      "type": "string",
      "minLength": 1,
      "required": true
    },
    "surname": {
      "type": "string",
      "minLength": 1,
      "required": true
    },
    "email": {
      "required": true,
      "type": "email"
    },
    "startYear": {
      "type": "integer",
      "required": true
    },
    "position": {
      "model": "position"
    }
  }
}
