{
  "connection": "default",
  "collectionName": "posts",
  "info": {
    "name": "post",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "createdAt",
      "updatedAt"
    ],
    "comment": ""
  },
  "attributes": {
    "title": {
      "minLength": 1,
      "required": true,
      "type": "string"
    },
    "summary": {
      "minLength": 1,
      "required": true,
      "type": "text",
      "maxLength": 250
    },
    "content": {
      "minLength": 1,
      "required": true,
      "type": "richtext"
    },
    "poster": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    }
  }
}
